{
  "name": "vvlv",
  "version": "1.0.2",
  "description": "Virtual-list component build with vue and rxjs",
  "author": "sxd",
  "license": "MIT",
  "repository": "shaoxiong789/vvlv",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
