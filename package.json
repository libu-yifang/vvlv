    "vue-styled-components": "^1.4.5"
  },
  "devDependencies": {
    "@svgr/rollup": "^2.4.1",
    "@types/jest": "^23.1.5",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-jsx-v-model": "^2.0.3",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
